    "native": {
        "upsip": "127.0.0.1",
        "upsport": "3551",
        "pollingInterval": 10000,
        "upsStates": [{
                "id": "hostname",
                "upsId": "HOSTNAME",
                "name": "Hostname of computer running apcupsd",
                "type": "string",
                "role": "state"
            },
            {
                "id": "version",
                "upsId": "VERSION",
                "name": "apcupsd version number, date and operating system",
                "type": "string",
                "role": "state"
            },
            {
                "id": "ups-name",
                "upsId": "UPSNAME",
                "name": "UPS name from configuration file (dumb) or EEPROM (smart)",
                "type": "string",
                "role": "state"
            },
            {
                "id": "cable",
                "upsId": "CABLE",
                "name": "Cable type specified in the configuration file",
                "type": "string",
                "role": "state"
            },
            {
                "id": "driver",
                "upsId": "DRIVER",
                "name": "Driver type",
                "type": "string",
                "role": "state"
            },
            {
                "id": "ups-mode",
                "upsId": "UPSMODE",
                "name": "Mode in which UPS is operating",
                "type": "string",
                "role": "state"
            },
            {
                "id": "startTime",
                "upsId": "STARTTIME",
                "name": "Date and time apcupsd was started",
                "type": "string",
                "role": "date"
            },
            {
                "id": "model",
                "upsId": "MODEL",
                "name": "UPS model derived from UPS information",
                "type": "string",
                "role": "state"
            },
            {
                "id": "status",
                "upsId": "STATUS",
                "name": "UPS status (online, charging, on battery etc)",
                "type": "string",
                "role": "state"
            },
            {
                "id": "lineVoltage",
                "upsId": "LINEV",
                "name": "Current input line voltage",
                "type": "number",
                "role": "value.voltage",
                "unit": "V"
            },
            {
                "id": "load",
                "upsId": "LOADPCT",
                "name": "Percentage of UPS load capacity used as estimated by UPS",
                "type": "number",
                "role": "state",
                "unit": "%"
            },
            {
                "id": "batteryCharge",
                "upsId": "BCHARGE",
                "name": "Current battery capacity charge",
                "type": "number",
                "role": "state",
                "unit": "%"
            },
            {
                "id": "timeLeft",
                "upsId": "TIMELEFT",
                "name": "Remaining runtime left on battery as estimated by the UPS",
                "type": "number",
                "role": "state",
                "unit": "min"
            },
            {
                "id": "minBatteryCharge",
                "upsId": "MBATTCHG",
                "name": "Min battery charge % (BCHARGE) required for system shutdown",
                "type": "number",
                "role": "state",
                "unit": "%"
            },
            {
                "id": "minBatteryRuntime",
                "upsId": "MINTIMEL",
                "name": "Min battery runtime (MINUTES) required for system shutdown",
                "type": "number",
                "role": "state",
                "unit": "min"
            },
            {
                "id": "maxBatteryRuntime",
                "upsId": "MAXTIME",
                "name": "Max battery runtime (TIMEOUT) after which system is shutdown",
                "type": "number",
                "role": "state",
                "unit": "min"
            },
            {
                "id": "sensitivity",
                "upsId": "SENSE",
                "name": "Current UPS sensitivity setting for voltage fluctuations",
                "type": "string",
                "role": "state"
            },
            {
                "id": "lowSwitchVoltage",
                "upsId": "LOTRANS",
                "name": "Input line voltage below which UPS will switch to battery",
                "type": "number",
                "role": "value.voltage",
                "unit": "V"
            },
            {
                "id": "highSwitchVoltage",
                "upsId": "HITRANS",
                "name": "Input line voltage above which UPS will switch to battery",
                "type": "number",
                "role": "value.voltage",
                "unit": "V"
            },
            {
                "id": "alarmDelay",
                "upsId": "ALARMDEL",
                "name": "Delay period before UPS starts sounding alarm",
                "type": "string",
                "role": "state"
            },
            {
                "id": "batteryVoltage",
                "upsId": "BATTV",
                "name": "Current battery voltage",
                "type": "number",
                "role": "value.voltage",
                "unit": "V"
            },
            {
                "id": "transferBatteryReason",
                "upsId": "LASTXFER",
                "name": "Reason for last transfer to battery since apcupsd startup",
                "type": "string",
                "role": "state"
            },
            {
                "id": "transferBatteryCount",
                "upsId": "NUMXFERS",
                "name": "Number of transfers to battery since apcupsd startupe",
                "type": "number",
                "role": "state"
            },
            {
                "id": "lastTransferToBattery",
                "upsId": "XONBATT",
                "name": "Date, time of last transfer to battery since apcupsd startup",
                "type": "string",
                "role": "date"
            },
            {
                "id": "timeOnBattery",
                "upsId": "TONBATT",
                "name": "Seconds currently on battery",
                "type": "number",
                "role": "state",
                "unit": "sec"
            },
            {
                "id": "totalTimeOnBattery",
                "upsId": "CUMONBATT",
                "name": "Cumulative seconds on battery since apcupsd startup",
                "type": "number",
                "role": "state",
                "unit": "sec"
            },
            {
                "id": "lastTransferToLine",
                "upsId": "XOFFBATT",
                "name": "Date, time of last transfer off battery since apcupsd startup",
                "type": "string",
                "role": "date"
            },
            {
                "id": "lastTestDate",
                "upsId": "LASTSTEST",
                "name": "Date, time of last test",
                "type": "string",
                "role": "date"
            },
            {
                "id": "selftestResult",
                "upsId": "SELFTEST",
                "name": "Self test result",
                "type": "string",
                "role": "state"
            },
            {
                "id": "upsStatusHex",
                "upsId": "STATFLAG",
                "name": "UPS status flag in hex",
                "type": "string",
                "role": "state"
            },
            {
                "id": "serialNo",
                "upsId": "SERIALNO",
                "name": "UPS serial number",
                "type": "string",
                "role": "state"
            },
            {
                "id": "nominalLineVoltage",
                "upsId": "NOMINV",
                "name": "Nominal line voltage",
                "type": "number",
                "role": "value.voltage",
                "unit": "V"
            },
            {
                "id": "nominalBatteryVoltage",
                "upsId": "NOMBATTV",
                "name": "Nominal battery voltage",
                "type": "number",
                "role": "value.voltage",
                "unit": "V"
            },
            {
                "id": "nominalUPSPower",
                "upsId": "NOMPOWER",
                "name": "Nominal power output in watts",
                "type": "number",
                "role": "value.power",
                "unit": "W"
            },
            {
                "id": "firmware",
                "upsId": "FIRMWARE",
                "name": "UPS firmware version",
                "type": "string",
                "role": "state"
            }
        ]
    },
    "objects": [],
    "instanceObjects": [{
            "_id": "info",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id": "info.connection",
            "type": "state",
            "common": {
                "role": "indicator.connected",
                "name": "If APCUSB pooling is active",
                "type": "boolean",
                "read": true,
                "write": false,
                "def": false
            },
            "native": {}
        },
        {
            "_id": "info.UPSHost",
            "type": "state",
            "common": {
                "role": "state",
                "name": "UPS host",
                "type": "string",
                "read": true,
                "write": false,
                "def": false
            },
            "native": {}
        },
        {
            "_id": "info.UPSPort",
            "type": "state",
            "common": {
                "role": "state",
                "name": "UPS port",
                "type": "number",
                "read": true,
                "write": false,
                "def": false
            },
            "native": {}
        }
    ]
}