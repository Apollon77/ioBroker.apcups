                "role": "state"
            },
            {
                "id": "cable",
                "upsId": "CABLE",
                "name": "Cable type specified in the configuration file",
                "type": "string",
                "role": "state"
            },
            {
                "id": "driver",
                "upsId": "DRIVER",
                "name": "Driver type",
                "type": "string",
                "role": "state"
            },
