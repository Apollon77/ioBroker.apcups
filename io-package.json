        "title": "APC UPS connector",
        "titleLang": {
            "en": "APC UPS connector",
            "de": "APC-USV-Anschluss",
            "ru": "APC UPS адаптер",
            "uk": "APC UPS адаптер",
            "pt": "Conector UPS APC",
            "nl": "APC UPS-connector",
