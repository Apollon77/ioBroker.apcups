            {
                "id": "firmware",
                "upsId": "FIRMWARE",
                "name": "UPS firmware version",
                "type": "string",
                "role": "state"
            },
            {
                "id": "endApc",
                "upsId": "ENDAPC",
                "name": "Date and time of request to apcupsd",
                "type": "string",
                "role": "date"
            },
            {
                "id": "battDate",
                "upsId": "BATTDATE",
                "name": "The change date of the battery",
                "type": "string",
                "role": "date"
            }
