                "role": "date"
            },
            {
                "id": "lastTestDate",
                "upsId": "LASTSTEST",
                "name": "Date, time of last test",
                "type": "string",
                "role": "date"
            },
            {
                "id": "selftestResult",
                "upsId": "SELFTEST",
                "name": "Self test result",
                "type": "string",
                "role": "state"
            },
            {
                "id": "upsStatusHex",
                "upsId": "STATFLAG",
                "name": "UPS status flag in hex",
                "type": "string",
                "role": "state"
            },
            {
                "id": "serialNo",
                "upsId": "SERIALNO",
                "name": "UPS serial number",
                "type": "string",
                "role": "state"
            },
            {
                "id": "nominalLineVoltage",
                "upsId": "NOMINV",
                "name": "Nominal line voltage",
