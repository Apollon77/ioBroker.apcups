            }
        },
        "dependencies": [
            {
                "js-controller": ">=4.0.0"
            }
        ]
    },
    "native": {
        "upsip": "127.0.0.1",
        "upsport": "3551",
        "pollingInterval": 10000,
        "upsStates": [
